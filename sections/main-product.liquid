                .gift_box + .gift_box {
                    border-top: 1px solid #aaa;
                    padding-top: 10px;
                    margin-top: 10px;
                }
                .gift-box-image{
                  width: 60px;
                }
                .gift-box-heading {
                    font-size: 20px;
                    font-weight: bold;
                    margin-bottom: 10px;
                    color: #000;
                }
                 .gift-box-title {
                    margin: 0 0 5px 0;
                    color:{{ block.settings.siez_color }} ;
                    font-size: 18px;
                 } 

                .gift-box-price {
                   color:{{ block.settings.siez_color }} ; 
                    font-size: 20px;
                    font-weight: bold;
                }
                .gift-box-price .price-at {
                    {% comment %} opacity: 0.5; {% endcomment %}
                    text-decoration: line-through;
                    margin-right: 10px;
                }
                span.price-zore {
                    font-weight: bold;
                }
                /* Hide the default checkbox */
                  .gift-box_label input[type="checkbox"] {
                      display: none;
                  }

                  /* Custom checkbox styling */
                  .gift-box_label {
                      position: relative;
                      display: inline-block;
                      cursor: pointer;
                  }

                  .custom-checkbox {
                    width: 3rem;
                    height: 3rem;
                    border: 2px solid #000;
                    border-radius: 3px;
                    display: inline-block;
                    background-color: #fff;
                    transition: all 0.3s ease;
                  }

                  /* When checkbox is checked */
                  .gift-box_label input[type="checkbox"]:checked + .custom-checkbox {
                      background-color: rgba(var(--color-button)); /* Green background when checked */
                      border-color: var(--color-button);
                      transform: scale(1.2); /* Animate scaling */
                  }

                  /* Add checkmark when checked */
                  .gift-box_label input[type="checkbox"]:checked + .custom-checkbox::before {
                    content: '';
                    position: absolute;
                    left: 0.8rem;
                    top: 0.2rem;
                    width: 1rem;
                    height: 1.5rem;
                    border: solid white;
                    border-width: 0 2px 2px 0;
                    transform: rotate(45deg);
                  }
                  .gift_box{
                    display: flex;
                    align-items: center;
                    justify-content: space-between;
                  }
                  /* Hover effect */
                  .gift-box_label:hover .custom-checkbox {
                      border-color: var(--color-button);
                  }
                  .gift-box_bing h4{
                      margin: 0 0 1rem 0;
                  }
                  .gift-box_bing ul li{
                      list-style-type:none;
                      color: {{ block.settings.siez_color }};
                  }
                  .gift-box_bing ul{
                      margin: 0;
                      padding: 0;
                  }
