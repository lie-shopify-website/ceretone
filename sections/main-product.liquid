                .newcountdownspan {
                  background-color: {{ block.settings.timebody_color }};
                  color: {{ block.settings.time_color }};
                  font-weight: 500;
                  font-size: 20px;
                  padding: 10px;
                  border-radius: 10px;
                }
                .newcountdownspancolor {
                  color: {{ block.settings.time_colorx }};
                  margin-top: 5px;
                  font-size: 1.4rem;
                }
                @media(max-width: 768px) {
                  .newcountdown_big {
                    display: block;
                  }
